{
  "compilerOptions": {
    // Target latest version of ECMAScript.
    "target": "esnext",
    // Search under node_modules for non-relative imports.
    "moduleResolution": "node",
    "module": "esnext",
    // Import non-ES modules as default imports.
    "esModuleInterop": true,
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": "./",
    "allowJs": true
  }
}
